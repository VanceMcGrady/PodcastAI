{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/vite"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/audio/(.*)",
      "dest": "/api/audio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}